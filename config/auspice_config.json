      },
      {
        "key": "country",
        "title": "Country",
        "type": "categorical"
      },
      {
        "key": "ncbiGeoRegion",
        "title": "Region",
        "type": "categorical"
      },
      {
        "key": "ncbiVirusTaxId",
        "title": "VirusTaxonId",
        "type": "categorical"
      },
      {
        "key": "clade_membership",
        "title": "Clade",
